<Script>Window.SetSize(Application.GetWndHandle(), 898, 535);
Window.SetPos(Application.GetWndHandle(), Window.GetPos(Application.GetWndHandle()).X, System.GetDisplayInfo().Height / 2 - Window.GetSize(Application.GetWndHandle()).Height / 2);

Transparency("DownloadAndInstall");

if(TestConnection(ClientURL, 3) == true)then
	Paragraph.SetText("stage1_information", ".Net Framework: 4.8 or later Required disc space: "..String.GetFormattedSize(GetSizeCheckMethod(ClientURL), FMTSIZE_AUTOMATIC, true).." Free disc space: "..String.GetFormattedSize(Math.Floor(Drive.GetFreeSpace(_SourceDrive)*1024*1048576/1024), FMTSIZE_AUTOMATIC, false));
else
	Dialog.Message("FATAL ERROR", "Unable to connect to the Server.\r\nCheck your internet connection or Contact an Administrator.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	Window.Close(Application.GetWndHandle(), CLOSEWND_TERMINATE);
end</Script>
