<Args/>
<Script>local State = Button.GetText(this);

if(State == "NEXT")then
	local GoDownload = (false);
	
	if(Folder.DoesExist(InstallFolder))then
		GoDownload = (true);
	else
		Folder.Create(InstallFolder);
		local CreateFolderError = Application.GetLastError();
		if(CreateFolderError ~= 0)then
			Dialog.Message("FATAL ERROR", _tblErrorMessages[CreateFolderError].."\r\nCannot continue, download canceled.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
			Application.Exit(0);
		else
			GoDownload = (true);
		end
	end
	
	if(GoDownload == true)then
		Paragraph.SetVisible("stage1_message", false);
		Input.SetVisible("stage1_input", false);
		Paragraph.SetVisible("stage1_selectfolder", false);
		Paragraph.SetVisible("stage1_information", false);
		
		Button.SetVisible("stage1_createdesktopicon", false);
		Paragraph.SetVisible("stage1_createdesktopicontext", false);
		
		Button.SetVisible("stage1_createstartmenuicon", false);
		Paragraph.SetVisible("stage1_createstartmenuicontext", false);
		
		Button.SetVisible("stage1_launchgameafterinstallation", false);
		Paragraph.SetVisible("stage1_launchgameafterinstallationtext", false);
		
		Image.Load("background", "AutoPlay\\Images\\background_1.png");
		Paragraph.SetVisible("stage2_percentage", true);
		Paragraph.SetVisible("stage2_downloadstatus", true);
		
		Button.SetText(this, "CANCEL");
		------------------------------------------------------------------------
		DownloadCheckMethod(ClientURL, InstallFolder.."temp.zip");
		local DownloadError = Application.GetLastError();
		if(DownloadError ~= 0 and DownloadError ~= 2508 and DownloadError ~= 1027)then
			UpdateStatus("Error");
			Dialog.Message("FATAL ERROR", _tblErrorMessages[DownloadError].."\r\nCannot continue, download canceled.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
			Application.Exit(0);
		elseif(DownloadError == 2508 or DownloadError == 1027)then
			Application.Exit(0);
		else
			--local Start = (os.clock());
			UpdateStatus("Please Wait...");
			ZipEx.Extract(InstallFolder.."temp.zip", { "*.*" }, InstallFolder, "", "ZipCallback");
			local ZipError = Application.GetLastError();
			if(ZipError ~= 0 and ZipError ~= 1027)then
				UpdateStatus("Error");
				Dialog.Message("FATAL ERROR", _tblErrorMessages[ZipError].."\r\nCannot continue, download canceled.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
				Application.Exit(0);
			elseif(ZipError == 1027)then
				Application.Exit(0);
			else
				--Dialog.Message("PROCESS FINISHED", "Process took:"..os.date("%M:%S", os.clock()-Start), MB_OK, MB_ICONNONE, MB_DEFBUTTON1);
				UpdateStatus("Deleting residue...");
				File.Delete(InstallFolder.."temp.zip", false, false, false, nil);
				
				if(File.DoesExist(InstallFolder.."LC.exe"))then
					if(CreateDesktopIcon)then
						UpdateStatus("Creating icon on desktop...");
						Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), ServerName, InstallFolder.."LC.exe", "", "", "", 0, SW_SHOWNORMAL, nil, "", START_OPTION_NO_PIN);
					end
					
					if(CreateStartMenuIcon)then
						UpdateStatus("Creating icon on start menu...");
						Shell.CreateShortcut(Shell.GetFolder(SHF_STARTMENU).."\\"..ServerName, ServerName, InstallFolder.."LC.exe", "", "", "", 0, SW_SHOWNORMAL, nil, "", START_OPTION_DEFAULT);
					end
				end
				
				local RegKey = ("SOFTWARE\\WOW6432Node\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\"..ServerName);
				
				UpdateStatus("Creating Uninstall process...");
				TextFile.WriteFromString(InstallFolder.."Uninstall.bat", [[
					@echo off
					title ]]..ServerName..[[ Uninstaller
					echo Stopping LC.exe...
					taskkill /f /im LC.exe
					
					echo Stopping Nksp.exe...
					taskkill /f /im Nksp.exe
					
					echo Removing game client files...
					set ClientFolder=%~dp0
					
					cd /d %ClientFolder%
					
					for /F "delims=" %%i in ('dir /b') do (
						if not "%%i"=="Uninstall.bat" (
							rd "%%i" /s /q || del "%%i" /s /q
							echo Deleting: %%i
						)
					)
					
					echo Removing Shortcut from Desktop...
					del /s /q "%USERPROFILE%\Desktop\]]..ServerName..[[.lnk"
					
					echo Removing Shortcut from Start Menu...
					rd /s /q "%USERPROFILE%\AppData\Roaming\Microsoft\Windows\Start Menu\]]..ServerName..[["
					
					echo Removing registry entry...
					reg delete "HKEY_LOCAL_MACHINE]].."\\"..RegKey..[[" /va /f
					
					echo msgbox "]]..ServerName..[[ was uninstalled successfully!" &gt; %tmp%\tmp.vbs
					cscript /nologo %tmp%\tmp.vbs		
					del %tmp%\tmp.vbs
					
					del "Uninstall.bat"
				]], false);
				
				UpdateStatus("Creating entry on registry...");
				Registry.CreateKey(HKEY_LOCAL_MACHINE, RegKey);
				--Comments
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "DisplayName", ServerName, REG_SZ);
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "Publisher", ServerName, REG_SZ);
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "URLInfoAbout", URLInfoAbout, REG_SZ);
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "HelpLink", HelpLink, REG_SZ);
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "DisplayIcon", InstallFolder.."LC.exe", REG_SZ);
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "UninstallString", InstallFolder.."Uninstall.bat", REG_SZ);
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "NoModify", "00000001", REG_DWORD);
				Registry.SetValue(HKEY_LOCAL_MACHINE, RegKey, "NoRepair", "00000001", REG_DWORD);
				
				UpdateStatus("Installing complements...");
				Shell.Execute("AutoPlay\\Docs\\vcredist_2010_x86.exe", "open", "/q", "", SW_SHOWNORMAL, true);
				Shell.Execute("AutoPlay\\Docs\\dxwebsetup.exe", "open", "/q", "", SW_SHOWNORMAL, true);
				UpdateStatus("Installation Completed!");
				
				if(LaunchGameAfterInstallation)then
					Shell.Execute(InstallFolder.."LC.exe", "open", "", "", SW_SHOWNORMAL, false);
				end
				
				Dialog.Message(ServerName, ServerName.." was installed successfully!", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
				
				Application.Exit(0);
			end
		end
	end
elseif(State == "CANCEL")then
	local Ret = Dialog.Message("Confirmation", "Sure you want to cancel the process?", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
	if(Ret == 7)then
		Application.Exit(0);
	end
end</Script>
