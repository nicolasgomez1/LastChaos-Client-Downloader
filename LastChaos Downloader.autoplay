<Script>http = require("socket.http");

SettingsFile = (_SourceFolder.."\\config.ini");

function GetConfig(Section, Value)
	return INIFile.GetValue(SettingsFile, Section, Value);
end

ClientURL = GetConfig("Settings", "ClientLink");
ServerName = GetConfig("Settings", "ServerName");
URLInfoAbout = GetConfig("Settings", "URLInfoAbout");
HelpLink = GetConfig("Settings", "HelpLink");
CreateDesktopIcon = (true);
CreateStartMenuIcon = (true);
LaunchGameAfterInstallation = (true);
InstallFolder = ("");

function TestConnection(sURL, nTimeout)
	if(nTimeout and type(nTimeout) == "number")then
		http.TIMEOUT = nTimeout;
	end
	local a, b = http.request({url=sURL, method="HEAD"});
	if(a == 1)then
		return true;
	else
		return false;
	end
end

function DownloadCheckMethod(Url, Path)
	if(String.Left(Url, 5) == "https")then
		HTTP.DownloadSecure(Url, Path, MODE_BINARY, 3, 443, nil, nil, DownloadCallback);
	else
		HTTP.Download(Url, Path, MODE_BINARY, 3, 80, nil, nil, DownloadCallback);
	end
end

function GetSizeCheckMethod(Url)
	if(String.Left(Url, 5) == "https")then
		return HTTP.GetFileSizeSecure(Url, MODE_BINARY, 3, 443, nil, nil, nil);
	else
		return HTTP.GetFileSize(Url, MODE_BINARY, 3, 80, nil, nil, nil);
	end
end

function DownloadCheckMethod(Url, Path)
	if(String.Left(Url, 5) == "https")then
		HTTP.DownloadSecure(Url, Path, MODE_BINARY, 1, 443, nil, nil, DownloadCallback);
	else
		HTTP.Download(Url, Path, MODE_BINARY, 1, 80, nil, nil, DownloadCallback);
	end
end

function UpdateStatus(Status, Percent)
	if(Status == "Completed")then
		Paragraph.SetText("stage2_downloadstatus", "Download Completed!");
		Image.SetSize("stage2_progressbar", 541, 42);

		TaskBarList.SetProgressState(TBPF_NOPROGRESS);
		--TaskBarList.SetOverlayIconFromLoadFile(LR_SHARED, "", 0);--######################podria refinar un poco el codigo del launcher, desactivar esto que creo que era cuando usaba un icono de update rotatorio en la barra de tareas, y sacar la llamada a socket que no es necesaria, tambien poner un windows.close(terminate) en los fatal error que haya antes de que se muestre la ventana, si es que los hay. en settings podria poner pausa musica.
	elseif(Status == "Error")then
		Paragraph.SetText("stage2_downloadstatus", "Download stopped!");
		
		TaskBarList.SetProgressState(TBPF_ERROR);
		TaskBarList.SetOverlayIcon(IDI_ERROR);
	else
		Paragraph.SetText("stage2_downloadstatus", Status);
	end
end

function QueryAllowProjectClose()
	Cancel = (true);
	return true;
end

function UninstallCallback(Source, Deleted, Total)
	Paragraph.SetText("FileList", Paragraph.GetText("FileList").."\r\n"..Source);
	
	Paragraph.SetText("stage2_percentage", (Deleted/Total*100).."%");
	
	Image.SetSize("stage2_progressbar", Total-Deleted*541/100, 42);
	
	if(Deleted == Total)then
		return false;
	else
		return true;
	end
end

function ZipCallback(DestinationPath, Percentage, Status)
	if(Status == ZIP_STATUS_MAJOR)then
		if(Cancel == true)then
			return false;
		end
		
		Paragraph.SetText("stage2_percentage", Percentage.."%");
		
		Image.SetSize("stage2_progressbar", Percentage*541/100, 42);
		
		TaskBarList.SetProgressValue(Percentage, 100);
	end
	
	if(Status == ZIP_STATUS_MINOR)then
		Application.Sleep(.01);
		
		if(Cancel == true)then
			return false;
		end
		
		local Split = String.SplitPath(DestinationPath);
		Paragraph.SetText("stage2_downloadstatus", "Unzipping File: "..Split.Filename..Split.Extension.." ("..Percentage.."%)");
	end
	
	if(Cancel == true)then
		return false;
	end
	
	return true;
end

function SecondsToClock(sSeconds)
	local nSeconds = String.ToNumber(sSeconds)
	if(nSeconds == 0)then
		return "00 Hours 00 Minutes 00 Seconds";
	else
		local nHours = string.format("%02.f", Math.Floor(nSeconds/3600));
		local nMins = string.format("%02.f", Math.Floor(nSeconds/60 - (nHours*60)));
		local nSecs = string.format("%02.f", Math.Floor(nSeconds - nHours*3600 - nMins *60));
		return nHours.." Hours "..nMins.." Minutes "..nSecs.." Seconds";
	end
end

function DownloadCallback(Downloaded, Total, TransferRate, SecondLeft, SecondsLeftFormat, Message)
	if(Message == "")then
		local Percent = Math.Floor(Downloaded/Total*100);
		
		Paragraph.SetText("stage2_percentage", Percent.."%");

		Image.SetSize("stage2_progressbar", Percent*541/100, 42);
		
		Paragraph.SetText("stage2_downloadstatus", string.format("REMAINING: %s  %s (%s/s)", SecondsToClock(SecondLeft), String.GetFormattedSize(Total-Downloaded, FMTSIZE_AUTOMATIC, false), String.GetFormattedSize(TransferRate*1024, FMTSIZE_AUTOMATIC, true)));
		
		TaskBarList.SetProgressValue(Percent, 100);
	end
	
	if(Cancel == true)then
		return false;
	end
	
	return true;
end

function Transparency(Page)
	local WS_EX_LAYERED = (524288);
	local msg = DLL.CallFunction(_SystemFolder.."\\user32.dll", "GetWindowLongA", Application.GetWndHandle()..", -20", DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);
	msg = (msg + WS_EX_LAYERED);
	DLL.CallFunction(_SystemFolder.."\\user32.dll", "SetWindowLongA", Application.GetWndHandle()..", -20, "..msg, DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);
	DLL.CallFunction(_SystemFolder.."\\user32.dll", "SetLayeredWindowAttributes", Application.GetWndHandle()..", "..Application.GetPageProperties(Page).BackgroundColor..", 255, 3", DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);
end</Script>
